{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.dcreativo.ch/chi-sono",
      "permanent": true,
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ]
}
